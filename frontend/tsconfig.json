{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@reduxjs/toolkit": ["./node_modules/@reduxjs/toolkit"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
